{
  "compilerOptions": {
    "outDir": "./public/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "baseUrl":"./",
    "esModuleInterop": true
  },
  "include": [
    "./src/**/*"
  ],
  "paths": {
    "@src/*": [
      "src/*"
    ]
  }
}